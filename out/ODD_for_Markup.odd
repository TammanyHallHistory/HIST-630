<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title>Title</title>
      </titleStmt>
      <publicationStmt>
        <p>Publication Information</p>
      </publicationStmt>
      <sourceDesc>
        <p>Information about the source</p>
      </sourceDesc>
    </fileDesc>
  </teiHeader>
  <text>
    <body>
      <schemaSpec ident="Markup_George_Washington_Plinkitt" start="TEI">
        <moduleRef key="header"/>
        <moduleRef key="core" include="title"/>
        <moduleRef key="tei"/>
        <moduleRef key="textstructure"/>
        <moduleRef key="namesdates" />
      </schemaSpec>
      <elementSpec module="namesdates" ident="persName" mode="change">
        <content>
          <sequence preserveOrder="true">
          <elementRef key="forename"/>
          <elementRef key="surname"/>
          </sequence>
        </content>
        <attList>
          <attDef ident="type" mode="change" usage="req">
            <datatype>
              <dataRef key="teidata.enumerated"/>
            </datatype>
            <valList>
              <valItem ident="polOrg">
                <gloss>Political Organization</gloss>
                <desc>A faction of the Republican or Democratic parties of New York City</desc>
              </valItem>
                <valItem ident="polOffice">
                  <gloss>Political Office</gloss>
                  <desc>Indicates that this person holds or held this political office</desc>
              </valItem>
              <valItem ident="Irish">
                <gloss>Irish</gloss>
                <desc>Any term that indicates the referrent is of Irish or Irish American descent</desc>
                </valItem>
                <valItem ident="American">
                  <gloss>American</gloss>
                  <desc>Any term that indicates the referrent identifies as a native born American without immigrant identity</desc>
                  </valItem>
                  <valItem ident="journ">
                    <gloss>Journalist</gloss>
                    <desc>A member of the New York City Press corps</desc>
              </valItem>
            </valList>
          </attDef>
        </attList>
      </elementSpec>
      <elementSpec ident="placeName" module="namesdates" mode="change">
        <content>
          <textNode/>
        </content>
        <attList>
          <attDef ident="ref" mode="change" usage="req"></attDef>
        </attList>
      </elementSpec>
      <elementSpec ident="orgName" module="namesdates" mode="change">
        <content>
          <textNode/>
        </content>
        <attList>
          <attDef ident="ref" mode="change" usage="req"></attDef>
        </attList>
      </elementSpec>
      <elementSpec ident="p" module="core" mode="change">
        <content>
          <sequence preserveOrder="false">
            <elementRef key="persName" minOccurs="0" maxOccurs="unbounded"/>
            <elementRef key="placeName" minOccurs="0" maxOccurs="unbounded"/>
            <elementRef key="orgName" minOccurs="0" maxOccurs="unbounded"/>
            <textNode/>
          </sequence>
        </content>
      </elementSpec>
    </body>
  </text>
</TEI>
